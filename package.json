{
  "name": "jeopardy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:docker": "docker compose -f docker-compose.dev.yml up",
    "dev:docker:build": "docker compose -f docker-compose.dev.yml up --build",
    "dev:docker:down": "docker compose -f docker-compose.dev.yml down",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "validate": "npm run lint && npm run typecheck",
    "test": "vitest",
    "test:run": "vitest run",
    "db:setup": "npm run db:generate && npm run db:migrate && npm run db:create-indexes && npm run db:seed:embeddings",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:reset": "prisma migrate reset --force",
    "db:push": "prisma db push",
    "db:seed": "ts-node --project tsconfig.json src/scripts/seed-database.ts",
    "db:seed:achievements": "ts-node --project tsconfig.json src/scripts/seed-achievements.ts",
    "db:seed:embeddings": "ts-node --project tsconfig.json src/scripts/seed-knowledge-embeddings.ts",
    "db:create-indexes": "ts-node --project tsconfig.json src/scripts/create-vector-indexes.ts",
    "db:fetch": "ts-node --project tsconfig.json src/scripts/fetch-jeopardy-data.ts",
    "db:backfill": "ts-node --project tsconfig.json src/scripts/backfill-jarchive.ts",
    "db:update-triple-stumpers": "ts-node --project tsconfig.json src/scripts/update-triple-stumpers.ts",
    "db:pre-generate-challenges": "ts-node --project tsconfig.json src/scripts/pre-generate-daily-challenges.ts",
    "prepare": "npm run db:generate"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.35.5",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@prisma/client": "^6.0.1",
    "@tanstack/react-query": "^5.12.2",
    "@types/bad-words": "^3.0.3",
    "@types/cheerio": "^0.22.35",
    "@types/node-cron": "^3.0.11",
    "@types/react-datepicker": "^6.2.0",
    "axios": "^1.7.8",
    "bad-words": "^4.0.0",
    "cheerio": "^1.0.0",
    "date-fns": "^4.1.0",
    "nanoid": "^5.1.6",
    "next": "^14.2.25",
    "node-cron": "^4.2.1",
    "node-html-parser": "^6.1.13",
    "openai": "^4.73.0",
    "pgvector": "^0.2.0",
    "react": "^18",
    "react-datepicker": "^7.5.0",
    "react-dom": "^18",
    "react-hot-toast": "^2.4.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.2.25",
    "postcss": "^8",
    "prisma": "^6.0.1",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^5",
    "vitest": "^4.0.14"
  }
}
